class rectangle {
    node*      parent;
    rectangle* next;
    int x;
    int y;
    int w;
    int h;

    void rebound(int x, int y, int w, int h) {
        this->x = x;
        // ...
    }
};


class node {
    node*      parent;
    rectangle* contained;
    node*      left;
    node*      right;
};


